##### PATH additions #####
            # Bundler Binstubs  # ~/bin   # MacPorts     # MacPorts      # Postgres                        # Cargo (Rust)    #####
export PATH=.git/safe/../../bin:$HOME/bin:/opt/local/bin:/opt/local/sbin:/usr/local/opt/postgresql@9.6/bin/:$HOME/.cargo/bin:$PATH

